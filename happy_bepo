xkb_keymap {
	xkb_keycodes  { include "xfree86+aliases(azerty)"	};
	xkb_types     { include "complete"	};
	xkb_compat    { include "complete"	};
	xkb_symbols   { include "pc"
			#include "level3(ralt_switch)"
 			include "level5(rctrl_switch)"


key <TLDE> { [ asciitilde ] };

key <AE01> { [ 1, ampersand ] };
key <AE02> { [ 2, asciitilde ] };
key <AE03> { [ 3, numbersign ] };
key <AE04> { [ 4, apostrophe ] };
key <AE05> { [ 5, bracketleft ] };
key <AE06> { [ 6, bar ] };
key <AE07> { [ 7, none ] };
key <AE08> { [ 8, backslash ] };
key <AE09> { [ 9, none ] };
key <AE10> { [ 0, none ] };
key <AE11> { [ 0, none ] };
key <AE12> { [ 0, none ] };

key <AD01> { [ b, B, asterisk ] };
key <AD02> { [ eacute, Eacute, equal ] };
key <AD03> { [ p, P, semicolon ] };
key <AD04> { [ o, O,  minus ] };
key <AD05> { [ comma, plus, exclam, bar ] };

key <AD07> { [ y, Y ] };
key <AD08> { [ v, V, percent ] };
key <AD09> { [ d, D, parenleft, bracketleft ] };
key <AD10> { [ l, L, parenright, bracketright ] };
key <AD11> { [ j, J, dollar ] };
key <AD12> { [ k, K, percent ] };
key <AD13> { [ dollar, asciicircum ] };

key <AC01> { [ a, A, Home ] };
key <AC02> { [ u, U, Down, Next ] };
key <AC03> { [ i, I, Up, Prior ] };
key <AC04> { [ e, E, Return ] };
key <AC05> { [ period, colon, Tab, ampersand ] };

key <AC06> { [ Return ] };

key <AC07> { [ c, C, Delete, ccedilla ] };
key <AC08> { [ t, T, BackSpace ] };
key <AC09> { [ s, S, Left ] };
key <AC10> { [ r, R, Right ] };
key <AC11> { [ n, N, End ] };
key <AC13> { [ asciicircum, percent ] };

key <LFSH> { [ Shift_L ] };
key <LSGT> { [ Caps_Lock ] };
key <AB11> { [ Shift_R, none, none ] };
			
key <AB01> { [ x, X, at ] };
key <AB02> { [ egrave, none, less ] };
key <AB03> { [ w, W, greater ] };
key <AB04> { [ f, F, question, none ] };
		
key <AB06> { [ z, Z, none, none] };
key <AB07> { [ m, M, slash, backslash ] };
key <AB08> { [ q, Q, braceleft, none ] };
key <AB09> { [ g, G, braceright, none ] };
key <AB10> { [ h, H, asciitilde, none ] };

key <LEFT> { [ Left, leftarrow ] };
key <RGHT> { [ Right, rightarrow ] };
key <UP>   { [ Up, uparrow] };
key <DOWN> { [ Down, downarrow] };


key <RTRN> { [ ISO_Level3_Shift ] };
key <LCTL> { [ ISO_Level3_Shift ] };

#key <LALT> { [ Control_L, none, none ] };
#key <RALT> { [ Control_L, none, none ] };
#key <KPEN> { [ Control_R ] };
key <SPCE> { [ space, quotedbl, apostrophe, underscore ] };
key <LWIN> { [ Control_L, none ] };
key <RWIN> { [ Control_R, none ] };

};

	xkb_geometry  { include "pc(pc105)"	};
};
